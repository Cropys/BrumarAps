    <script type="text/javascript">
        function sendMessage() {
            var url = "/Home/ContactUs";
            var name = $("#txtName").val();
            var email = $("#txtEmail").val();
            var message = $("#txtMessage").val();
            var doPost = true;

            if (email == "") {
                doPost = false;
                $("#emailError").css("style", "display:block");
            }
            if (name == "") {
                doPost = false;
                $("#nameError").css("style", "display:block");
            }
            if (message == "") {
                doPost = false;
                $("#messageError").css("style", "display:block");
            }

            if (doPost) {
                $.post(url, { Name: name, Email: email, Message: message }, function (data) {
                    if (data == "OK") {
                        alert("Email successful");
                    }
                    if (data == "ERROR") {
                        alert("Something went wrong");
                    }
                    alert("response:" + data);
                });
            }

        }

    </script>

